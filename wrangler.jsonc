{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "ao3-theme-generator",
  "compatibility_date": "2025-09-27",
  "main": "src/server.ts",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "observability": {
    "enabled": true
  },
  "assets": {
    "run_worker_first": true,
    "directory": "dist/assets",
    "binding": "ASSETS"
  }
}