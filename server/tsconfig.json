{
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.worker.tsbuildinfo",
    "target": "ES2023",
    "lib": ["ES2023"],
    "module": "ESNext",
    "skipLibCheck": true,
    "moduleResolution": "bundler",
    "noEmit": true,
    "strict": true,
    "erasableSyntaxOnly": true,
    "allowImportingTsExtensions": false,
    "noUncheckedSideEffectImports": true,
    "types": [
      "@types/node"
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "src",
    "worker-configuration.d.ts"
  ]
}
